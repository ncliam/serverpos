<?xml version="1.0"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="view_pos_pos_form">
            <field name="name">nc.pos.order</field>
            <field name="model">pos.order</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='lines']//tree" position="attributes">
                    <attribute name="editable"></attribute>
                </xpath>
                <xpath expr="//field[@name='lines']//tree//field[@name='qty']" position="after">
                    <field name="uos"/>
                    <field name="uos_qty"/>
                </xpath>
                <xpath expr="//field[@name='lines']//form//group//field[@name='qty']" position="after">
                    <field name="uos"/>
                    <field name="uos_qty"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
